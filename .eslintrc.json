{
    "extends": "eslint:recommended",
    "plugins": ["standard"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module",
        "allowImportExportEverywhere": true
    },
    "env": {
        "node": true,
        "es6": true,
        "jest": true
    },
    "rules": {
        "space-before-function-paren": 0,
        "new-cap": 0
    }
}
